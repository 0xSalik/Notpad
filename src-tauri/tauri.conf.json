{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm run build -- --base /",
    "beforeDevCommand": "pnpm run dev",
    "frontendDist": "../www",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": ["app", "deb", "dmg", "msi", "rpm", "nsis"],
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "Notpad",
  "mainBinaryName": "Notpad",
  "version": "1.0.0-3",
  "identifier": "tech.rahif.notpad",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "width": 800,
        "minHeight": 450,
        "minWidth": 350,
        "resizable": true,
        "title": "Notpad"
      }
    ],
    "security": {
      "csp": null
    }
  }
}
